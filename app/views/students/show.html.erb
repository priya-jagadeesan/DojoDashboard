<h1><%= @dojo.branch %>'s <%= @student.first_name %> <%= @student.last_name %></h1>
<p><b>Email : </b><%= @student.email %></p>
    <% if @student.present? %>
    <table id="customers">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
        <tr>
          <td><%= @student.first_name %></td>
          <td><%= @student.last_name %></td>
          <td><%= @student.email %></td>
    </table><br><br>
    <% end %>
    <a href='/dojos'>Home</a><br><br>
    <a href='/dojos/<%= @dojo.id%>/students/<%= @student.id %>/edit'>Edit</a>
