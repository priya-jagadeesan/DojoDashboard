  <% if @dojo.present? %>
    <h2><%= @dojo.branch %></h2>  <a href='/dojos/<%= @dojo.id %>/edit'>Edit</a>
    <p> <b>Address :  </b><%= @dojo.street %></p>
    <p><b>City : </b><%= @dojo.city %>                    <b>State : </b><%= @dojo.state %>
    <br><br>
    <h3>Student Roster</h3>
    <% if @students.present? %>
    <table id="customers">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.first_name %></td>
          <td><%= student.last_name %></td>
          <td><%= student.email %></td>
          <td>
              <a href='/dojos/<%= @dojo.id %>/students/<%= student.id %>'>Show</a>
              <a href='/dojos/<%= @dojo.id%>/students/<%= student.id %>/edit'>Edit</a>
              <a href='/dojos/<%= @dojo.id%>/students/<%= student.id %>' data-method="delete">Destroy</a>
          </td>
      <% end %>
    </table><br><br>
    <% end %>
    <a href='/dojos'>Home</a><br><br>
    <a href='/dojos/<%= @dojo.id %>/students/new'>Add student</a>
  <% end %>